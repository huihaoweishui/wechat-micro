.page {
  font-size: 11pt;
  display: flex;
  flex-flow: column nowrap;
  justify-content: flex-start;
  align-content: space-between;
  overflow: hidden;
}

.headerLine {
  width: 100%;
  height: 2rpx;
  background-color: #eee;
}

.scrollItemHr {
  width: 100%;
  height: 2rpx;
  background-color: #eee;
  margin-top: 17rpx;
}

.headerEndLine {
  width: 100%;
  height: 2rpx;
  background-color: #eee;
}

.header {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: baseline;
}

.city {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: baseline;
  margin-left: 18rpx;
}

.hd {
  height: 13rpx;
  width: 13rpx;
  border-width: 3rpx 3rpx 0 0;
  border-style: solid;
  transform: rotate(135deg);
  margin-left: 10rpx;
  align-self: center;
}

.img {
  width: 30rpx;
  height: 30rpx;
}

.search {
  background-color: #e6e6e6;
  border-radius: 12rpx;
  font-weight: 100;
  color: grey;
  flex-grow: 2;
  margin-right: 10rpx;
  margin-left: 10rpx;
}

.searchImg {
  vertical-align: middle;
  margin-right: 10rpx;
  margin-left: 10rpx;
  margin-bottom: 5rpx;
}

/*module*/

.module {
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
  align-items: baseline;
}
.module navigator{
  width: 33%;
}
.moduleImg {
  width: 100%;
  height: 100%;
  text-align: center;
}

.contentImg {
  width: 60rpx;
  height: 60rpx;
}

.moduleItem {
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-around;
  align-items: center;
  margin-top: 20rpx;
}

.moduleName {
  margin-top: 20rpx;
}
/* cityshow */
.city2 {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 50px;
  background: white;
}
.cityScroll{
 background-color: white;
}
.titleText {
  background-color: #efeef4;
  font-size: 30rpx;
  font-weight: 100;
  color: grey;
  padding: 10rpx;
  padding-left: 25rpx;
}

.position {
  font-size: 30rpx;
  padding: 10rpx;
  padding-left: 25rpx;
}

.initial {
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
}

.initialItem {
  font-weight: 300;
  font-size: 22rpx;
  width: 25%;
  box-sizing: border-box;
  text-align: center;
  border-right: thin solid gainsboro;
  border-bottom: thin solid gainsboro;
  padding: 13rpx;
}