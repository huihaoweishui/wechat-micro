.orderView {
  width: 100%;
  height: 100%;
  background-color: #efeef4;
  display: flex;
  justify-content: center;
  flex-flow: column nowrap;
  align-items: center;
}

.noOrder {
  width: 200rpx;
  height: 200rpx;
  border: 1 solid #f00;
  border-radius: 100rpx;
  background-color: #e3e5ea;
}

.noOrderText {
  font-size: 25rpx;
  font-weight: 100;
  color: grey;
  margin-top: 30rpx;
  margin-bottom: 250rpx;
}
